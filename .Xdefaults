# Zoom control
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Control-Shift-Down: resize-font:smaller
URxvt.keysym.Control-Shift-Up:   resize-font:bigger

URxvt.intensityStyles: false
URxvt.visualBell: false
URxvt.cursorColor: #a6a492
URxvt.cursorBlink: true

URxvt.scrollstyle: xterm
URxvt.thickness: 1
URxvt.scrollColor: #3a404d

